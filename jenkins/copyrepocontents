pipeline {
    agent any

    stages {
        stage('Copy Repository Contents') {
            steps {
                // Checkout the source repository
                git url: 'https://github.com/Raghuds48/duplicate-repo-history.git', branch: 'master'

                // Add the destination repository as a remote
                sh 'git remote add destination https://github.com/Raghuds48/wisecow.git'

                // Push the contents to the destination repository
                sh 'git push -u destination master'
            }
        }
    }
}
